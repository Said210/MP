javascript:
  var ready;
  ready = function() {
  	$(window).scrollTop(1);
  };

  $(document).ready(ready);
  $(document).on('page:load', ready);

  $(window).scroll(function (event) {
      $(".section").each(function(){
      	doSc($(this),82);
      });
      $(".middle").each(function(){
      	doSc($(this),1);
      });
  });
  function doSc(elem, q){
  	var scroll = $(window).scrollTop();
  	if(scroll==0){scroll=1};

  	var space=$(elem).data("place");
  	var speed=$(elem).data("speed");
  	speed=0;
  	if(speed==undefined){speed=q;}else{}
  	if(space<2){
  		var s="calc("+100*space+"% + ";
  		var s2=(speed-scroll)/space;
  		s=s+s2+"px)";
  		$(elem).css("top",s);
  	}else{
  		var s="calc("+100*space*.75+"% + ";
  		var s2=(speed-scroll)/space;
  		
  		s=s+s2+"px)";
  		$(elem).css("top",s);
  	}
  }
